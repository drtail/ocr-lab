# Streamlit Secrets Configuration Example
# Copy this file to secrets.toml and fill in your API keys
# For Streamlit Cloud: Add these in the app settings under "Secrets"

# Veryfi Configuration
VERYFI_CLIENT_ID = "your_client_id"
VERYFI_CLIENT_SECRET = "your_client_secret"
VERYFI_USERNAME = "your_username"
VERYFI_API_KEY = "your_api_key"

# Taggun Configuration
TAGGUN_API_KEY = "your_api_key"

# Klippa Configuration
KLIPPA_API_KEY = "your_api_key"

# OpenAI (ChatGPT) Configuration
OPENAI_API_KEY = "your_api_key"

# Mistral Configuration
MISTRAL_API_KEY = "your_api_key"

# Google Gemini Configuration
GEMINI_API_KEY = "your_api_key"
